import org.jointheleague.graphical.robot.Robot;

public class Fase
{
	public static void main(String[] args)
	{
		Fase F = new Fase();
	}

	Fase()
	{
		Robot r = new Robot();
		eye1(r);
		eye2(r);
		mouth(r);
		moostash(r);
	}

	private void moostash(Robot r)
	{

	}

	private void mouth(Robot r)
	{

	}

	private void eye2(Robot r)
	{
		r.moveTo(400, 30);
		r.penDown();
		for (int i = 0; i < 360; i++)
		{
			r.move(2);
			r.turn(1);
		}
		r.penUp();
	}

	private void eye1(Robot r)
	{
		r.moveTo(100, 30);

	}
}
