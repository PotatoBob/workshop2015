import javax.swing.JButton;
import javax.swing.JFrame;
import javax.swing.JPanel;
import javax.swing.JTextField;

public class DeDRipOff {
	JFrame window;
	JPanel panel;
	JTextField CR;
	JTextField T;
	JButton four;
	JButton six;
	JButton eight;
	JButton ten;
	JButton twelve;
	JButton twenty;
	JButton clear;
	int x;
	int y;

	public static void main(String[] args) {
		new DeDRipOff();
	}

	DeDRipOff() {
		window = new JFrame("Dungeons and Dragons Ripoff");
		window.setVisible(true);
		panel = new JPanel();

		JTextField CR = new JTextField();
		CR.setText("" + x);
		JTextField T = new JTextField();
		// T.setText()
		JButton four = new JButton("4");
		four.setBounds(50, 50, 300, 100);
		JButton six = new JButton("6");
		JButton eight = new JButton("8");
		JButton ten = new JButton("10");
		JButton twelve = new JButton("12");
		JButton twenty = new JButton("20");
		JButton clear = new JButton("clear");
		panel.add(four);
		panel.add(six);
		panel.add(eight);
		panel.add(ten);
		panel.add(twelve);
		panel.add(twenty);
		panel.add(clear);
		//panel.add(CR);
		//panel.add(T);

		window.add(panel);
		window.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		window.pack();
	}
}
