
public class FibonacciCrap {
	int num1;
	int num2 = 1;
	int num3;

	public static void main(String[] args) {
		FibonacciCrap FC = new FibonacciCrap();
	}

	FibonacciCrap() {
		for (int i = 0; i < 100; i++) {
			num3 = num1 + num2;
			System.out.println(num3);
			num1 = num2 + num3;
			System.out.println(num1);
			num2 = num3 + num1;
			System.out.println(num2);
		}
	}
}
