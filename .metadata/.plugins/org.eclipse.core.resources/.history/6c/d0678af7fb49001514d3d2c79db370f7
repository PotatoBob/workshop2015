import org.jointheleague.graphical.robot.Robot;

public class robotMethod
{
	Robot Bob;
	Robot Bobe;
	public static void main(String[] args)
	{
		Robot Bob = new Robot();
		Robot Bobe = new Robot();
	}

	void drawSquareBob() {
	for (int i = 0; i < 4; i++)
	{
		Bob.penDown();
		Bob.move(100);
		Bob.turn(90);
	}
	}
}
